{
  "$schema": "../information-flow.schema.json",
  "name": "Todo App",
  "description": "A simple todo application for managing tasks",
  "version": "1.0.0",
  "timeline": [
    {
      "type": "state",
      "name": "Cart",
      "tick": 1,
      "sourcedFrom": ["OrderPlaced"],
      "example": {
        "orderId": "ord-123",
        "customerId": "cust-456",
        "amount": 99.99
      }
    },
    {
      "type": "actor",
      "name": "Customer",
      "tick": 2,
      "readsView": "OrderPlaced",
      "sendsCommand": "PlaceOrder"
    },
    {
      "type": "command",
      "name": "PlaceOrder",
      "tick": 3,
      "example": {
        "orderId": "ord-123",
        "customerId": "cust-456",
        "amount": 99.99
      }
    },
    {
      "type": "event",
      "name": "OrderPlaced",
      "producedBy": "PlaceOrder-3",
      "tick": 10,
      "example": {
        "orderId": "ord-123",
        "customerId": "cust-456",
        "amount": 99.99
      }
    },
    {
      "type": "event",
      "name": "OrderPlaced",
      "producedBy": "PlaceOrder-3",
      "tick": 11,
      "example": {
        "orderId": "ord-123",
        "customerId": "cust-456",
        "amount": 10.99
      }
    }

  ]
}
